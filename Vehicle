public class Vehicle {
    private String name;
    private int weight;

    public Vehicle(String name) {
        this.name = name;
        System.out.print("Weight: ");
    }

    public String getName() {
        return name;
    }

    public int getWeight() {
        return weight;
    }

    public void setWeight(int weight) {
        this.weight = weight;
    }
}
